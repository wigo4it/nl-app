      <categorie id="1202" key="14a6cda5-ed24-4c06-9e41-8b8cbf7ea299" parentID="1144" level="3" creatorID="1" sortOrder="3" createDate="2018-04-11T09:47:52" updateDate="2018-04-11T09:47:52" nodeName="Gezondheid" urlName="gezondheid" path="-1,1143,1144,1202" isDoc="" nodeType="1151" creatorName="Karoy Bandi" writerName="Karoy Bandi" writerID="1" template="1150" nodeTypeAlias="categorie" isPublished="true">
